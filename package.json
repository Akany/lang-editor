{
  "name": "language-ditor",
  "version": "1.0.0",
  "description": "App to help edit languages",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --env.mode=development",
    "build": "webpack --config webpack.config.js --env.mode=production",
    "start": "ELECTRON_ENV=dev electron .",
    "dist": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "build": {
    "appId": "language_editor",
    "mac": {
      "category": "json file editor"
    },
    "files": [
      "main.js",
      "dist/**/*"
    ],
    "directories": {
      "output": "build"
    }
  },
  "author": "",
  "repository": "",
  "license": "",
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "bulma": "^0.7.1",
    "css-loader": "^1.0.0",
    "electron": "^3.0.0-beta.3",
    "electron-builder": "^20.27.1",
    "electron-reload": "^1.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
